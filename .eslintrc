{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "google",
    "rules": {
        "require-jsdoc": 0,
        "no-unused-vars": "warn",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 0
            }
        ],
        "no-underscore-dangle": [
            0
        ],
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error",
                    "log"
                ]
            }
        ],
        "max-len": "off",
        "camelcase": "off",
        "object-curly-spacing": ["error", "never"]
    }
}
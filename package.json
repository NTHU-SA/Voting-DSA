{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "docker-compose -f ./docker-compose.dev.yml up -d && env NODE_ENV=dev && node app.js",
    "start": "node app.js",
    "init": "npx sequelize init",
    "create": "npx db:create",
    "create:prod": "npx sequelize db:create --env production",
    "migration": "npx sequelize migration:create --name create-examples-table",
    "migration:prod": "npx sequelize migration:create --name create-examples-table --env production",
    "migrate": "npx sequelize db:migrate",
    "migrate:prod": "npx sequelize db:migrate --env production",
    "seeder": "npx sequelize seed:create --name init-users",
    "seeder:prod": "npx sequelize seed:create --name init-users --env production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.3.1",
    "mongoose": "^5.9.10",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "sequelize-cli": "^5.5.1"
  }
}
